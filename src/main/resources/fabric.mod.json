{
  "schemaVersion": 1,
  "id": "ds",
  "version": "${version}",
  "name": "Deep Silence",
  "description": "A mod that brings Phasmophobia to Minecraft.",
  "authors": [
    "Rastin1st",
    "Antilop_22"
  ],
  "contact": {
    "sources": "https://github.com/Alekseev-Mikhail/deep-silence"
  },
  "license": "CC0-1.0",
  "icon": "assets/ds/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "io.github.DeepSilenceMod",
        "adapter": "kotlin"
      }
    ],
    "client": [
      {
        "value": "io.github.DeepSilenceModClient",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "ds.mixins.json",
    {
      "config": "ds.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.14.22",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.9.0",
    "geckolib": ">=4.2.2"
  },
  "suggests": {
    "another-mod": "*"
  }
}