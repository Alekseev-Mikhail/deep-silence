{
  "schemaVersion": 1,
  "id": "phasmophobia",
  "version": "${version}",
  "name": "Phasmophobia",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Rastin1st",
    "Antilop_22"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/phasmophobia/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "io.github.ExampleMod",
        "adapter": "kotlin"
      }
    ],
    "client": [
      {
        "value": "io.github.ExampleModClient",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "phasmophobia.mixins.json",
    {
      "config": "phasmophobia.client.mixins.json",
      "environment": "client"
    }
